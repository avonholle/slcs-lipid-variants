	pmcid = {PMC2762151},
	keywords = {Anemia, Iron-Deficiency, Body Height, Body Weight, Chile, Dietary Supplements, Female, Food Additives, Growth, Humans, Infant, Iron, Longitudinal Studies, Male, Reference Values, Retrospective Studies, Urban Population, World Health, World Health Organization}
}

