	pmid = {11805004},
	keywords = {Autoanalysis, Cholesterol, {LDL}, Diabetes Mellitus, Electrophoresis, Humans, Kidney Failure, Chronic, Liver Failure, Practice Guidelines as Topic, Ultracentrifugation},
