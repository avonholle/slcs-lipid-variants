	booktitle = {Cochrane Database of Systematic Reviews},
	publisher = {John Wiley \& Sons, Ltd},
	author = {Ebrahim, Shah and Taylor, Fiona and Ward, Kirsten and Beswick, Andrew and Burke, Margaret and Davey Smith, George},
	editor = {{The Cochrane Collaboration}},
	urldate = {2016-06-01},
	date = {2011-01-19},
