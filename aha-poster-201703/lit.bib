	keywords = {Age Factors, Animal Nutritional Physiological Phenomena, Animals, Animals, Newborn, Biomarkers, Birth Weight, Blood Glucose, Diet, High-Fat, Energy Intake, Fatty Acids, Nonesterified, Female, Fetal Growth Retardation, Gestational Age, Liver, Male, Maternal Nutritional Physiological Phenomena, Nutritional Status, Organ Size, Pregnancy, Prenatal Exposure Delayed Effects, Rats, Wistar, Starvation, Time Factors, Triglycerides},
